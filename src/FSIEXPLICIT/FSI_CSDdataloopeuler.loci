	prelude { 
	
	(*$CSDnodes_it{n,it+1}).resize((*$CSDnodes_ic).size1(),3) ; std::fill( (*$CSDnodes_it{n,it+1}).data().begin(), (*$CSDnodes_it{n,it+1}).data().end(), 0. ); 
